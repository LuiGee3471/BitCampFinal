<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.or.bit.dao.TeacherCourseDao">
  <select id="selectTeacherCourse" resultType = "kr.or.bit.model.TeacherCourse">
  	SELECT teacher_username, course_id
  	FROM teacher_course
  	WHERE course_id = #{course_id}
  </select>
</mapper>